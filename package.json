  "name": "DiscordJS-Handler",
  "version": "1.0.0",
  "description": "Command Handler by Nico Laeser",
  "main": "/src/index.js",
  "scripts": {
    "build": "tsc && tsc-alias && copyfiles -u 1 \"src/**/*.json\" \"src/api/public/**/*\" dist",
    "start": "node dist/cluster.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/cluster.ts",
    "deploy": "ts-node -r tsconfig-paths/register src/scripts/deploy.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md,yml,yaml}\"\n",
    "cu": "npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolaeser/DiscordJS-Handler.git"
  },
  "keywords": [
